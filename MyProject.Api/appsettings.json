{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": "Server=YOUR_SERVER;Database=YOUR_DB_NAME;User ID=YOUR_SQL_USER;Password=YOUR_SQL_PASSWORD;TrustServerCertificate=True;"
  },

  "JwtSettings": {
    "Issuer": "YourApiIssuer",
    "Audience": "YourApiClient",
    "Key": "YOUR_SUPER_SECRET_AND_LONG_SIGNING_KEY_GOES_HERE_123456"
  },

  "GoogleSettings": {
    "ClientId": "YOUR_GOOGLE_CLIENT_ID_GOES_HERE",
    "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET_GOES_HERE"
  }
}
